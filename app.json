{
  "expo": {
    "scheme": "pocketseq",
    "userInterfaceStyle": "light",
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ic_adaptive.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.sequence.pocketseq",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM",
        "WRITE_EXTERNAL_STORAGE",
        "AD_ID"
      ],
      "versionCode": 44,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD2fBfh5KYKd4Mcyw_iN90EQCfGT917fKE"
        }
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "eas": {
        "projectId": "0d25da57-059d-4fd1-ad34-23d9debffae5"
      }
    },
    "icon": "./assets/images/ic_app.png",
    "ios": {
      "bundleIdentifier": "com.sequence.pocketseq",
      "buildNumber": "73",
      "supportsTablet": false,
      "infoPlist": {
        "NSLocationUsageDescription": "位置情報へのアクセスを許可して、近くの施設を見つけられるようにします。",
        "CFBundleLocalizations": [
          "ja"
        ],
        "CFBundleDevelopmentRegion": "ja",
        "CFBundleAllowMixedLocalizations": true,
        "NSCameraUsageDescription": "$(PRODUCT_NAME)がカードのスキャンのためカメラにアクセスできるようにします"
      },
      "config": {
        "googleMapsApiKey": "AIzaSyDijYEeUIa9FCakUPipxEUAKjKaAv1DsXo"
      },
      "entitlements": {
        "com.apple.developer.applesignin": [
          "default"
        ]
      },
      "usesAppleSignIn": true
    },
    "name": "PocketseQ",
    "orientation": "portrait",
    "owner": "jrgavinash",
    "plugins": [
      "sentry-expo",
      [
        "expo-image-picker",
        {
          "photosPermission": "アプリは写真にアクセスします。"
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "33.0.0",
            "enableProguardInReleaseBuilds": true
          },
          "ios": {
            "deploymentTarget": "13.0"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "$(PRODUCT_NAME)がカメラにアクセスできるようにする"
        }
      ],
      "expo-apple-authentication"
    ],
    "slug": "pocketseq",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "version": "1.0.1"
  }
}
